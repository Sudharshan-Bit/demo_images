{
  "builds": [
    {
      "src": "image/wsgi.py",  
      "use": "@vercel/python",   
      "config": {
        "runtime": "python3.9"   
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",  
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",   
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",         
      "dest": "image/wsgi.py"
    }
  ]
}
